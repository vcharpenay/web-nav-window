{
    "manifest_version": 2,
    "version": "0.2",
    "name": "Web Navigation Window",
    "browser_action": {
      "default_title": "Web Navigation Window",
      "default_icon": {
          "16": "icon-16.png",
          "32": "icon-32.png",
          "64": "icon-64.png"
      }
    },
    "permissions": [
        "history",
        "activeTab"
    ],
    "web_accessible_resources": [
        "ui.html",
        "util.js",
        "navigation.js",
        "network.js",
        "lib/vis-network.min.js",
        "lib/uri.all.min.js"
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    }
}